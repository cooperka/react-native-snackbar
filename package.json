{
  "name": "react-native-snackbar",
  "version": "0.4.3",
  "description": "Material-design \"Snackbar\" component for Android and iOS",
  "main": "lib/index.js",
  "types": "src/index.d.ts",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "RNSnackbar.podspec"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint src example --ext .js,.jsx",
    "flow": "flow src",
    "clean": "rm -rf lib",
    "build": "npm run clean && babel src --out-dir lib --ignore src/__tests__",
    "prepublish": "npm run build"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/"
    ],
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cooperka/react-native-snackbar.git"
  },
  "keywords": [
    "react-native",
    "component",
    "ios",
    "android",
    "material design",
    "snackbar",
    "snack bar"
  ],
  "author": "Azendoo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cooperka/react-native-snackbar/issues"
  },
  "homepage": "https://github.com/cooperka/react-native-snackbar",
  "peerDependencies": {
    "react": ">=15.1 || >=16.0.0-alpha.6",
    "react-native": ">=0.40"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.0.1",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-cooperka": "0.2.7",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "eslint-plugin-react-native": "3.1.0",
    "flow-bin": "0.57.3",
    "jest": "21.2.1",
    "react": "16.0.0",
    "react-native": "0.49.3"
  }
}
