{
  "name": "react-native-snackbar",
  "version": "2.4.0",
  "description": "Material Design \"Snackbar\" component for Android and iOS.",
  "main": "lib/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:types": "tsc",
    "lint": "eslint . --ext .js,.jsx",
    "flow": "flow src",
    "clean": "rm -rf lib",
    "build": "yarn run clean && babel src --out-dir lib --ignore src/__tests__",
    "prepublish": "yarn run build"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "node_modules/react-native/.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "node_modules/@react-native/.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/example/"
    ],
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cooperka/react-native-snackbar.git"
  },
  "keywords": [
    "react-native",
    "component",
    "ios",
    "android",
    "material design",
    "snackbar",
    "snack bar"
  ],
  "author": "Azendoo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cooperka/react-native-snackbar/issues"
  },
  "homepage": "https://github.com/cooperka/react-native-snackbar",
  "resolutions": {
    "react-native/@jest/create-cache-key-function": "27.0.6"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-native": ">=0.60"
  },
  "devDependencies": {
    "@babel/cli": "7.14.5",
    "@babel/core": "7.14.6",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.0.6",
    "eslint": "7.30.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-cooperka": "1.0.4",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-native": "3.11.0",
    "jest": "27.0.6",
    "metro-react-native-babel-preset": "0.66.1",
    "react": "17.0.2",
    "react-native": "0.64.2",
    "typescript": "4.3.5"
  }
}
